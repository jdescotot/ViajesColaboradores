@model ViajesColaboradores.Models.ViajeModel

<h4>ViajeModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Index">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Creado" class="control-label"></label>
                <input asp-for="Creado" class="form-control" />
                <span asp-validation-for="Creado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Colaborador" class="control-label"></label>
                <input asp-for="Colaborador" class="form-control" />
                <span asp-validation-for="Colaborador" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sucursal" class="control-label"></label>
                <input asp-for="Sucursal" class="form-control" />
                <span asp-validation-for="Sucursal" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Transportista" class="control-label"></label>
                <input asp-for="Transportista" class="form-control" />
                <span asp-validation-for="Transportista" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Administrador" class="control-label"></label>
                <input asp-for="Administrador" class="form-control" />
                <span asp-validation-for="Administrador" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Distancia" class="control-label"></label>
                <input asp-for="Distancia" class="form-control" />
                <span asp-validation-for="Distancia" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="ViajePosible" /> @Html.DisplayNameFor(model => model.ViajePosible)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="List">Lista de Viajes</a>
</div>
<div>
    <a asp-action="Create/ColaboratorModels/Views">Crear Usuario</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
